[main]
name = mount-fstab
description = Parse and mount all the devices listed in /etc/fstab
type = oneshot

[start]
build = auto
execute =(
    foreground { s6-echo -- “Starting oneshot: mount-fstab” }
    
    foreground { mount --all }
    
    s6-echo -- “Done oneshot: mount-fstab”
)

[options]
dependencies = [ fsck mount-rwfs ]

